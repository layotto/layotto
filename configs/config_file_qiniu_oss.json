                {
                  "type": "grpc",
                  "config": {
                    "server_name": "runtime",
                    "grpc_config": {
                      "hellos": {
                        "helloworld": {
                          "hello": "greeting"
                        }
                      },
                      "file": {
                        "qiniuOSS": {
                          "metadata": [
                            {
                              "endpoint": "<require>",
                              "accessKeyID": "<require>",
                              "accessKeySecret": "<require>",
                              "bucket": "<require>",
                              "private": false
                            }
                          ]
                        }
                      }
                    }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "pprof": {
    "debug": true,
    "port_value": 34902
  }
