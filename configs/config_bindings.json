{
  "servers": [
    {
      "default_log_path": "stdout",
      "default_log_level": "DEBUG",
      "listeners": [
        {
          "name": "grpc",
          "address": "127.0.0.1:34904",
          "bind_port": true,
          "filter_chains": [
            {
              "filters": [
                {
                  "type": "grpc",
                  "config": {
                    "server_name": "runtime",
                    "grpc_config": {
                      "hellos": {
                        "quick_start_demo": {
                          "type": "helloworld",
                          "hello": "greeting"
                        }
                      },
                      "bindings": {
                        "bindings_demo": {
                          "type": "http",
                          "metadata":
                            {
                                "url": "https://github.com/mosn/layotto"
                            }
                        }
                      }
                    }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "pprof": {
    "debug": true,
    "port_value": 34902
  }
}
